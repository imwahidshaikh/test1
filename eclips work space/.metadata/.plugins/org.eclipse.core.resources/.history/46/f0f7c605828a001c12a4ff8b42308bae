package prjct1;

import java.util.Scanner;

public class factorial {

	public static void main(String[] args) {
Scanner s = new Scanner(System.in);
int no = s.nextInt();
System.out.println("eneter any no");

			int a=1;
			for ( int i=no ; i>=1;i--) {
				
		a=i*a;
	
			}
			System.out.println(a);
			

	}

}
